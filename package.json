{
  "name": "electron-vite-start",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:mac-arm": "npm run build && electron-builder --mac --arm64",
    "build:linux": "npm run build && electron-builder --linux",
    "electron:generate-icons": "uosjs-icon-builder --input=./resources/icon.png --output=.icons --flatten",
    "init:module": "node scripts/module",
    "mock": "mocker ./mock"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "electron-events": "^0.3.2",
    "electron-log": "^5.1.2",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.7",
    "fluent-ffmpeg": "^2.1.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@icon-park/vue-next": "^1.4.2",
    "@rushstack/eslint-patch": "^1.7.1",
    "@tomjs/electron-devtools-installer": "^2.2.1",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/node": "^18.19.9",
    "@uosjs/icon-builder": "^1.0.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "electron": "^30.0.2",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.2.0",
    "element-plus": "^2.6.3",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "lodash-es": "^4.17.21",
    "mocker-api": "^2.9.10",
    "mockjs": "^1.1.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.4",
    "sass": "^1.72.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.3.3",
    "vite": "^5.2.7",
    "vue": "^3.4.15",
    "vue-router": "^4.3.2",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "ajv": ">=7.0.0"
  }
}
